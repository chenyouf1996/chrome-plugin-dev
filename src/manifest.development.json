{
    "manifest_version": 3,
    "name": "html2css-struct",
    "description": "html标签转scss类名结构",
    "version": "0.0.1",
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "popup.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}